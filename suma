#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	int A,B,C,D,E,F;
    cout <<"ingrese un valor"<<endl;
	cin>>A;
	cout <<"ingrese otro valor"<<endl;
	cin>>B;
	cout << "Ingrese otro valor"<<endl;
	cin>>C;
	cout << "Ingrese otro valor"<<endl;
	cin>>D;
	cout << "Ingrese otro valor"<<endl;
	cin>>E;
	F=(A+B-C)*D/E;
	cout<< "El Resultado es:"<<F;
	getch();
}
